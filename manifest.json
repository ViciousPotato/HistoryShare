{
  "name": "History Share",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Share your interests and find good stuff from others.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup" : "popup.html"
  },
  "background" : {"scripts" : ["urlcollector.js"]},
  "permissions": [
    "unlimitedStorage",
    "webNavigation",
    "tabs",
    "history"
  ]
}